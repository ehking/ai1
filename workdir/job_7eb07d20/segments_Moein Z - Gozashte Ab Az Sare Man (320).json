{
  "segments": [
    {
      "start": 0.0,
      "end": 6.8,
      "text": "میدونی توفه خوزش که کارهی نمود"
    },
    {
      "start": 6.8,
      "end": 10.8,
      "text": "بارش کردن"
    },
    {
      "start": 10.8,
      "end": 14.0,
      "text": "دارفه که قشگر دیرفدیم"
    },
    {
      "start": 14.0,
      "end": 16.8,
      "text": "نمی چی و چشگر دیرفدیم"
    },
    {
      "start": 16.8,
      "end": 19.2,
      "text": "میدون سیها نم خرابه"
    },
    {
      "start": 19.2,
      "end": 22.0,
      "text": "نمی بازلتش کردیرفدیم"
    },
    {
      "start": 22.0,
      "end": 24.8,
      "text": "هندل امه دست دو ده دم"
    },
    {
      "start": 24.8,
      "end": 27.2,
      "text": "امه تو لهش کردیرفدیم"
    },
    {
      "start": 27.2,
      "end": 30.0,
      "text": "شدم ای چی باسم ده هندم"
    },
    {
      "start": 30.0,
      "end": 32.4,
      "text": "بجارو به هش رو بیرا"
    },
    {
      "start": 32.4,
      "end": 36.0,
      "text": "خبت شتونون درو بیرا نمان نیزانقم"
    },
    {
      "start": 36.0,
      "end": 38.8,
      "text": "دار خونه گمانو گرزی باست"
    },
    {
      "start": 38.8,
      "end": 41.6,
      "text": "تو امتاین میشه دلت میگیری باست"
    },
    {
      "start": 41.6,
      "end": 44.0,
      "text": "با خونه گمان دو"
    },
    {
      "start": 44.0,
      "end": 46.4,
      "text": "تن های می تو بکسره"
    },
    {
      "start": 46.4,
      "end": 49.2,
      "text": "گیچی شاده می تو خکسره"
    },
    {
      "start": 49.2,
      "end": 52.0,
      "text": "بیابه بیناه نم خرابه"
    },
    {
      "start": 52.0,
      "end": 54.0,
      "text": "امه اینه نازه شکست"
    },
    {
      "start": 54.0,
      "end": 56.4,
      "text": "بزید گزاشت او باست"
    },
    {
      "start": 56.4,
      "end": 58.0,
      "text": "هنمان"
    },
    {
      "start": 58.0,
      "end": 60.8,
      "text": "چی می خوای از دل امان"
    },
    {
      "start": 60.8,
      "end": 63.6,
      "text": "خیمی چیزا تحصیره تو"
    },
    {
      "start": 63.6,
      "end": 65.6,
      "text": "که زی چیزا تحصیره من"
    },
    {
      "start": 65.6,
      "end": 67.6,
      "text": "گزاشت او باست"
    },
    {
      "start": 67.6,
      "end": 68.8,
      "text": "هنمان"
    },
    {
      "start": 68.8,
      "end": 70.0,
      "text": "گیچی و ماست"
    },
    {
      "start": 70.0,
      "end": 71.6,
      "text": "هنمان"
    },
    {
      "start": 71.6,
      "end": 72.8,
      "text": "های چی دل امه"
    },
    {
      "start": 72.8,
      "end": 74.4,
      "text": "سپوره"
    },
    {
      "start": 74.4,
      "end": 76.8,
      "text": "خالی درو برای من"
    },
    {
      "start": 84.0,
      "end": 86.48,
      "text": "باست"
    },
    {
      "start": 86.48,
      "end": 88.16,
      "text": "تا ایش"
    },
    {
      "start": 88.16,
      "end": 90.56,
      "text": "باست"
    },
    {
      "start": 90.56,
      "end": 93.36,
      "text": "دلتی"
    },
    {
      "start": 93.36,
      "end": 95.6,
      "text": "خیمی چیزا تحصیره من"
    },
    {
      "start": 95.6,
      "end": 97.6,
      "text": "گزاشت"
    },
    {
      "start": 97.6,
      "end": 100.0,
      "text": "باست"
    },
    {
      "start": 100.0,
      "end": 102.88,
      "text": "خیمی چیزا تحصیره من"
    },
    {
      "start": 102.88,
      "end": 104.56,
      "text": "گزاشت"
    },
    {
      "start": 104.56,
      "end": 106.56,
      "text": "باست"
    },
    {
      "start": 106.56,
      "end": 108.0,
      "text": "باست"
    },
    {
      "start": 108.0,
      "end": 110.0,
      "text": "باست"
    },
    {
      "start": 110.0,
      "end": 112.0,
      "text": "باست"
    },
    {
      "start": 112.0,
      "end": 117.0,
      "text": "با که غرمز نهیزنیم نسف شبابم تکس نمیزنیم"
    },
    {
      "start": 117.0,
      "end": 120.0,
      "text": "تمام شده ما قسم یادیم"
    },
    {
      "start": 120.0,
      "end": 124.0,
      "text": "دیگه بد گیر نمیده هابله و استاد کتا هاست"
    },
    {
      "start": 124.0,
      "end": 128.0,
      "text": "دیش اکتلا بیرنه کبا درزد بگم چی میگن با"
    },
    {
      "start": 128.0,
      "end": 131.0,
      "text": "ورد نمیشدارم جدی میگنده"
    },
    {
      "start": 131.0,
      "end": 134.0,
      "text": "دیگه بد گیر نمیده هابله و گجامیری"
    },
    {
      "start": 134.0,
      "end": 136.0,
      "text": "گجامی آی با چی یا کی هاست"
    },
    {
      "start": 136.0,
      "end": 139.0,
      "text": "خبر کردی خیلی کچی دست با"
    },
    {
      "start": 139.0,
      "end": 141.0,
      "text": "ورد نمیشدارم جدی میگنده"
    },
    {
      "start": 141.0,
      "end": 145.0,
      "text": "گزاشت او با سرمان"
    },
    {
      "start": 145.0,
      "end": 147.0,
      "text": "چی میخوای از دل امان"
    },
    {
      "start": 147.0,
      "end": 150.0,
      "text": "خیلی چیزا تحصیر تو"
    },
    {
      "start": 150.0,
      "end": 152.0,
      "text": "کذی چیزا تحصیر امان"
    },
    {
      "start": 152.0,
      "end": 156.0,
      "text": "گزاشت او با سرمان"
    },
    {
      "start": 156.0,
      "end": 158.0,
      "text": "دیدی چیو ما سرمان"
    },
    {
      "start": 158.0,
      "end": 161.0,
      "text": "هاچی دلاما سا پوره"
    },
    {
      "start": 161.0,
      "end": 164.0,
      "text": "خالی دورا برمان"
    },
    {
      "start": 164.0,
      "end": 168.0,
      "text": "گفتم جاو شدت شده دوگ تر شده"
    },
    {
      "start": 168.0,
      "end": 172.0,
      "text": "ای کاشلال بودم وارفی"
    },
    {
      "start": 172.0,
      "end": 174.0,
      "text": "نمیزده"
    },
    {
      "start": 174.0,
      "end": 175.0,
      "text": "امان"
    },
    {
      "start": 175.0,
      "end": 179.0,
      "text": "هاول مراه از شره رای ام بو نخای دوگ دید"
    },
    {
      "start": 179.0,
      "end": 181.0,
      "text": "مهن"
    },
    {
      "start": 181.0,
      "end": 184.0,
      "text": "پوشت شدری که نخا هم گفت"
    },
    {
      "start": 184.0,
      "end": 185.0,
      "text": "میمیره"
    },
    {
      "start": 185.0,
      "end": 187.0,
      "text": "اشخی که یه روزی به تو داده"
    },
    {
      "start": 187.0,
      "end": 190.0,
      "text": "ماما گین که تو یه خابت بینه"
    },
    {
      "start": 190.0,
      "end": 193.0,
      "text": "با باره یه روزی منو تا ها"
    },
    {
      "start": 193.0,
      "end": 196.0,
      "text": "ماما گین که دیر خواکت بینه"
    },
    {
      "start": 196.0,
      "end": 198.0,
      "text": "با باره ام نمیشه یه شفه ابدم"
    },
    {
      "start": 198.0,
      "end": 201.0,
      "text": "به تونه قادم بسیه"
    },
    {
      "start": 201.0,
      "end": 206.0,
      "text": "چه جوری شده تو دلتو مات منو با حول خراب ببینه"
    },
    {
      "start": 206.0,
      "end": 209.0,
      "text": "با سچی من دلش ناید"
    },
    {
      "start": 209.0,
      "end": 211.0,
      "text": "که تو ها از دلش بری"
    },
    {
      "start": 211.0,
      "end": 214.0,
      "text": "تو همه یه داشن هم بودی"
    },
    {
      "start": 214.0,
      "end": 217.0,
      "text": "رسیدان داشن هم بیکی"
    },
    {
      "start": 217.0,
      "end": 220.0,
      "text": "با سچی من دلش ناید"
    },
    {
      "start": 220.0,
      "end": 223.0,
      "text": "دلتو بشکنه یکی"
    },
    {
      "start": 223.0,
      "end": 226.0,
      "text": "قلبي که ما لمن نشه"
    },
    {
      "start": 226.0,
      "end": 230.0,
      "text": "موهنی با شه ما لیکی"
    },
    {
      "start": 230.0,
      "end": 232.0,
      "text": "دارفه کی غش کردی رفتی"
    },
    {
      "start": 232.0,
      "end": 235.0,
      "text": "امه چی او چش کردی رفتی"
    },
    {
      "start": 235.0,
      "end": 238.0,
      "text": "بیدون سیها ها دم خرابه"
    },
    {
      "start": 238.0,
      "end": 241.0,
      "text": "امه بازلتش کردی رفتی"
    },
    {
      "start": 241.0,
      "end": 243.0,
      "text": "ایمندل امه دست ده دا ادن"
    },
    {
      "start": 243.0,
      "end": 246.0,
      "text": "امه تو لهش کردی رفتی"
    },
    {
      "start": 246.0,
      "end": 249.0,
      "text": "شو دمه چی با سم چه هم دم"
    },
    {
      "start": 249.0,
      "end": 251.0,
      "text": "دارو به هش کردی رفتی"
    },
    {
      "start": 251.0,
      "end": 253.0,
      "text": "با سر مر"
    },
    {
      "start": 253.0,
      "end": 257.0,
      "text": "تو چی میخوای از دل مر"
    },
    {
      "start": 257.0,
      "end": 259.0,
      "text": "خیبی چیزا تقصیر تایو"
    },
    {
      "start": 259.0,
      "end": 261.0,
      "text": "وزی چیزا تقصیر مر"
    },
    {
      "start": 261.0,
      "end": 263.0,
      "text": "تو سر مر"
    },
    {
      "start": 263.0,
      "end": 265.0,
      "text": "با سر مر"
    },
    {
      "start": 265.0,
      "end": 267.0,
      "text": "دیری چی مر"
    },
    {
      "start": 267.0,
      "end": 270.0,
      "text": "وزی دلا هر سمار"
    }
  ]
}