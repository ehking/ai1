{
  "segments": [
    {
      "start": 0.0,
      "end": 6.8,
      "text": "میدونی توفه خوزش که کارهی نمود"
    },
    {
      "start": 6.8,
      "end": 10.8,
      "text": "بارش کردن"
    },
    {
      "start": 10.8,
      "end": 14.0,
      "text": "دارفه که قشگر دیرفدیم"
    },
    {
      "start": 14.0,
      "end": 16.8,
      "text": "نمی چی و چشگر دیرفدیم"
    },
    {
      "start": 16.8,
      "end": 19.2,
      "text": "میدون سیها نم خرابه"
    },
    {
      "start": 19.2,
      "end": 22.0,
      "text": "نمی بازلتش کردیرفدیم"
    },
    {
      "start": 22.0,
      "end": 24.8,
      "text": "هندل امه دست دو ده دم"
    },
    {
      "start": 24.8,
      "end": 27.2,
      "text": "امه تو لهش کردیرفدیم"
    },
    {
      "start": 27.2,
      "end": 30.0,
      "text": "شدم ای چی باسم ده هندم"
    },
    {
      "start": 30.0,
      "end": 32.4,
      "text": "بجارو به هش رو بیرا"
    },
    {
      "start": 32.4,
      "end": 36.0,
      "text": "خبت شتونون درو بیرا نمان نیزانقم"
    },
    {
      "start": 36.0,
      "end": 38.8,
      "text": "دار خونه گمانو گرزی باست"
    },
    {
      "start": 38.8,
      "end": 41.6,
      "text": "تو امتاین میشه دلت میگیری باست"
    },
    {
      "start": 41.6,
      "end": 44.0,
      "text": "با خونه گمان دو"
    },
    {
      "start": 44.0,
      "end": 46.4,
      "text": "تن های می تو بکسره"
    },
    {
      "start": 46.4,
      "end": 49.2,
      "text": "گیچی شاده می تو خکسره"
    },
    {
      "start": 49.2,
      "end": 52.0,
      "text": "بیابه بیناه نم خرابه"
    },
    {
      "start": 52.0,
      "end": 54.0,
      "text": "امه اینه نازه شکست"
    },
    {
      "start": 54.0,
      "end": 56.4,
      "text": "بزید گزاشت او باست"
    },
    {
      "start": 56.4,
      "end": 58.0,
      "text": "هنمان"
    },
    {
      "start": 58.0,
      "end": 60.8,
      "text": "چی می خوای از دل امان"
    },
    {
      "start": 60.8,
      "end": 63.6,
      "text": "خیمی چیزا تحصیره تو"
    },
    {
      "start": 63.6,
      "end": 65.6,
      "text": "که زی چیزا تحصیره من"
    },
    {
      "start": 65.6,
      "end": 67.6,
      "text": "گزاشت او باست"
    },
    {
      "start": 67.6,
      "end": 68.8,
      "text": "هنمان"
    },
    {
      "start": 68.8,
      "end": 70.0,
      "text": "گیچی و ماست"
    },
    {
      "start": 70.0,
      "end": 71.6,
      "text": "هنمان"
    },
    {
      "start": 71.6,
      "end": 72.8,
      "text": "های چی دل امه"
    },
    {
      "start": 72.8,
      "end": 74.4,
      "text": "سپوره"
    },
    {
      "start": 74.4,
      "end": 76.8,
      "text": "خالی درو برای من"
    },
    {
      "start": 84.0,
      "end": 85.04,
      "text": "معرص bene"
    },
    {
      "start": 114.0,
      "end": 116.96,
      "text": "نسف شباب ام تکس نمیزنیم"
    },
    {
      "start": 116.96,
      "end": 119.96,
      "text": "تمام شده ما قسم ینیم"
    },
    {
      "start": 119.96,
      "end": 122.08,
      "text": "دیگه بد گیر نمیده"
    },
    {
      "start": 122.08,
      "end": 123.52,
      "text": "بلا و ساد کتا هاست"
    },
    {
      "start": 123.52,
      "end": 125.12,
      "text": "دیش اقتلا بیرنه"
    },
    {
      "start": 125.12,
      "end": 127.48,
      "text": "کی با در سد بگم چی میگن"
    },
    {
      "start": 127.48,
      "end": 130.68,
      "text": "با ورد نمیشدارم جدی میگن ده"
    },
    {
      "start": 130.68,
      "end": 133.08,
      "text": "دیگه بد گیر نمیگن"
    },
    {
      "start": 133.08,
      "end": 135.8,
      "text": "گجامی ری گجامی آی با چی یا کی هاست"
    },
    {
      "start": 135.8,
      "end": 137.28,
      "text": "خبر کردی"
    },
    {
      "start": 137.28,
      "end": 141.28,
      "text": "خیلی کچی دست با ورد نمیشدارم جدی میگن"
    },
    {
      "start": 141.28,
      "end": 144.96,
      "text": "گزاش ده آبا سره من"
    },
    {
      "start": 144.96,
      "end": 147.6,
      "text": "چی چی میخوای از دل امان"
    },
    {
      "start": 147.6,
      "end": 150.44,
      "text": "خیلی چیزا تحصیر تو"
    },
    {
      "start": 150.44,
      "end": 152.48,
      "text": "کذی چیزا تحصیر امان"
    },
    {
      "start": 152.48,
      "end": 155.88,
      "text": "گزاش ده آبا سره من"
    },
    {
      "start": 155.88,
      "end": 158.56,
      "text": "دیدی چیو ما سره من"
    },
    {
      "start": 158.56,
      "end": 161.28,
      "text": "ها چی دل امان سا پوره"
    },
    {
      "start": 161.28,
      "end": 164.08,
      "text": "خالی دوره بره من"
    },
    {
      "start": 164.08,
      "end": 168.84,
      "text": "گفنم جا و شبت شده دوگ تر شده"
    },
    {
      "start": 169.24,
      "end": 172.52,
      "text": "ای کا و شلال بودم وارفی"
    },
    {
      "start": 172.52,
      "end": 173.92000000000002,
      "text": "نمیزده"
    },
    {
      "start": 173.92000000000002,
      "end": 175.24,
      "text": "امان"
    },
    {
      "start": 175.24,
      "end": 179.8,
      "text": "هاول مراه از شره رایم دو نخای نود"
    },
    {
      "start": 179.8,
      "end": 181.2,
      "text": "مهن"
    },
    {
      "start": 181.2,
      "end": 185.52,
      "text": "پوشت شد ری که نخا هم گفت میمیره"
    },
    {
      "start": 185.52,
      "end": 187.56,
      "text": "اشخی که یه روزی به تو داده"
    },
    {
      "start": 187.56,
      "end": 190.84,
      "text": "ماما گین که تو یه خابه بینه"
    },
    {
      "start": 190.84,
      "end": 192.92000000000002,
      "text": "با باره یه روزی من و تا هم"
    },
    {
      "start": 192.92000000000002,
      "end": 196.4,
      "text": "ماما گین که دیر خاکه بینه"
    },
    {
      "start": 196.4,
      "end": 198.72,
      "text": "با باره ام نمیشه یه شبه ابدم"
    },
    {
      "start": 198.72,
      "end": 201.92000000000002,
      "text": "به تونه و قاده بس یه"
    },
    {
      "start": 201.92000000000002,
      "end": 203.96,
      "text": "چی جوری شده تو دله تو ماما"
    },
    {
      "start": 203.96,
      "end": 206.6,
      "text": "من و با هول خرابه بینه"
    },
    {
      "start": 206.6,
      "end": 209.4,
      "text": "و سچی من دلش ناید"
    },
    {
      "start": 209.4,
      "end": 211.88,
      "text": "که تو ها از دلش بری"
    },
    {
      "start": 211.88,
      "end": 214.72,
      "text": "تو همه یه داشن هم بودی"
    },
    {
      "start": 214.72,
      "end": 217.52,
      "text": "رسیدان داشن هم بیکی"
    },
    {
      "start": 217.52,
      "end": 220.20000000000002,
      "text": "و سچی من دلش ناید"
    },
    {
      "start": 220.20000000000002,
      "end": 222.92000000000002,
      "text": "دله تو بشکنه یه کی"
    },
    {
      "start": 222.92000000000002,
      "end": 225.6,
      "text": "قلبي که ما لی من نشه"
    },
    {
      "start": 225.6,
      "end": 229.68,
      "text": "موهنی با اوشه ما لیکی"
    },
    {
      "start": 229.68,
      "end": 232.4,
      "text": "دارفه کی غش کردی رفتی"
    },
    {
      "start": 232.4,
      "end": 235.07999999999998,
      "text": "همه چی و کش کردی رفتی"
    },
    {
      "start": 235.07999999999998,
      "end": 237.84,
      "text": "بیدون سیها هم خرابه"
    },
    {
      "start": 237.84,
      "end": 240.48,
      "text": "امه بازلتش کردی رفتی"
    },
    {
      "start": 240.48,
      "end": 243.28,
      "text": "ایمندل همه دست داده هم"
    },
    {
      "start": 243.28,
      "end": 245.88,
      "text": "امه تو لهش کردی رفتی"
    },
    {
      "start": 245.88,
      "end": 248.68,
      "text": "شده همه چی با سم چه هم نم"
    },
    {
      "start": 248.68,
      "end": 250.92,
      "text": "دجاره و بهش کردی رفتی"
    },
    {
      "start": 250.92,
      "end": 254.28,
      "text": "ده زاشه آبا سرمان"
    },
    {
      "start": 254.28,
      "end": 256.96,
      "text": "ده چی میخوای از نمار"
    },
    {
      "start": 256.96,
      "end": 259.68,
      "text": "خیبی چیزا تقسیر تارو"
    },
    {
      "start": 259.68,
      "end": 262.16,
      "text": "بزی چیزا تقسیر مرکوزه"
    },
    {
      "start": 262.16,
      "end": 265.16,
      "text": "شده آبا سرمان"
    },
    {
      "start": 265.16,
      "end": 267.88,
      "text": "دیری چی وقصرمان"
    },
    {
      "start": 267.88,
      "end": 270.68,
      "text": "و جیده نهار سماره"
    }
  ]
}