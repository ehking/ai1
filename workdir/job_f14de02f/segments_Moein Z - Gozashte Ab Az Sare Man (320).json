{
  "segments": [
    {
      "start": 0.0,
      "end": 6.8,
      "text": "میدونی توفه خوزش که کارهی نمود"
    },
    {
      "start": 6.8,
      "end": 10.8,
      "text": "بارش کردن"
    },
    {
      "start": 10.8,
      "end": 14.0,
      "text": "دارفه که قشگر دیرفدیم"
    },
    {
      "start": 14.0,
      "end": 16.8,
      "text": "نمی چی و چشگر دیرفدیم"
    },
    {
      "start": 16.8,
      "end": 19.2,
      "text": "میدون سیها نم خرابه"
    },
    {
      "start": 19.2,
      "end": 22.0,
      "text": "نمی بازلتش کردیرفدیم"
    },
    {
      "start": 22.0,
      "end": 24.8,
      "text": "هندل امه دست دو ده دم"
    },
    {
      "start": 24.8,
      "end": 27.2,
      "text": "امه تو لهش کردیرفدیم"
    },
    {
      "start": 27.2,
      "end": 30.0,
      "text": "شدم ای چی باسم ده هندم"
    },
    {
      "start": 30.0,
      "end": 32.4,
      "text": "بجارو به هش رو بیرا"
    },
    {
      "start": 32.4,
      "end": 36.0,
      "text": "خبت شتونون درو بیرا نمان نیزانقم"
    },
    {
      "start": 36.0,
      "end": 38.8,
      "text": "دار خونه گمانو گرزی باست"
    },
    {
      "start": 38.8,
      "end": 41.6,
      "text": "تو امتاین میشه دلت میگیری باست"
    },
    {
      "start": 41.6,
      "end": 44.0,
      "text": "با خونه گمان دو"
    },
    {
      "start": 44.0,
      "end": 46.4,
      "text": "تن های می تو بکسره"
    },
    {
      "start": 46.4,
      "end": 49.2,
      "text": "گیچی شاده می تو خکسره"
    },
    {
      "start": 49.2,
      "end": 52.0,
      "text": "بیابه بیناه نم خرابه"
    },
    {
      "start": 52.0,
      "end": 54.0,
      "text": "امه اینه نازه شکست"
    },
    {
      "start": 54.0,
      "end": 56.4,
      "text": "بزید گزاشت او باست"
    },
    {
      "start": 56.4,
      "end": 58.0,
      "text": "هنمان"
    },
    {
      "start": 58.0,
      "end": 60.8,
      "text": "چی می خوای از دل امان"
    },
    {
      "start": 60.8,
      "end": 63.6,
      "text": "خیمی چیزا تحصیره تو"
    },
    {
      "start": 63.6,
      "end": 65.6,
      "text": "که زی چیزا تحصیره من"
    },
    {
      "start": 65.6,
      "end": 67.6,
      "text": "گزاشت او باست"
    },
    {
      "start": 67.6,
      "end": 68.8,
      "text": "هنمان"
    },
    {
      "start": 68.8,
      "end": 70.0,
      "text": "گیچی و ماست"
    },
    {
      "start": 70.0,
      "end": 71.6,
      "text": "هنمان"
    },
    {
      "start": 71.6,
      "end": 72.8,
      "text": "های چی دل امه"
    },
    {
      "start": 72.8,
      "end": 74.4,
      "text": "سپوره"
    },
    {
      "start": 74.4,
      "end": 76.8,
      "text": "خالی درو برای من"
    },
    {
      "start": 84.0,
      "end": 90.8,
      "text": "دیگه تو بغل جا برامی"
    },
    {
      "start": 90.8,
      "end": 93.6,
      "text": "با رفتنت بال و برامی"
    },
    {
      "start": 93.6,
      "end": 96.0,
      "text": "دیگه می وریم تو عواشی"
    },
    {
      "start": 96.0,
      "end": 98.8,
      "text": "بید زین میزهن وقتی نواشی"
    },
    {
      "start": 98.8,
      "end": 102.0,
      "text": "دیگه بامست نمیزهنیم"
    },
    {
      "start": 102.0,
      "end": 104.0,
      "text": "با که غرمز نیزهن"
    },
    {
      "start": 104.0,
      "end": 106.0,
      "text": "با زیدیم امان"
    },
    {
      "start": 106.0,
      "end": 108.0,
      "text": "امان"
    },
    {
      "start": 108.0,
      "end": 110.0,
      "text": "امان"
    },
    {
      "start": 110.0,
      "end": 112.0,
      "text": "دیگه بامست نمیزهنیم"
    },
    {
      "start": 112.0,
      "end": 114.0,
      "text": "با که غرمز نیزهنیم"
    },
    {
      "start": 114.0,
      "end": 116.0,
      "text": "نسف شباب ام تکس نمیزهنیم"
    },
    {
      "start": 116.0,
      "end": 120.0,
      "text": "تمام شده ما قسم ینیم"
    },
    {
      "start": 120.0,
      "end": 122.0,
      "text": "دیگه بد گرنیم ده"
    },
    {
      "start": 122.0,
      "end": 124.0,
      "text": "بلا و سد کتا هاست"
    },
    {
      "start": 124.0,
      "end": 126.0,
      "text": "دیش اکتلا بیرنهر"
    },
    {
      "start": 126.0,
      "end": 128.0,
      "text": "کی با درزت بگم چی میگن با"
    },
    {
      "start": 128.0,
      "end": 130.0,
      "text": "ورد نمیشدار ام جدی میگن ده"
    },
    {
      "start": 130.0,
      "end": 132.0,
      "text": "دیگه بد گرنیم میگن"
    },
    {
      "start": 132.0,
      "end": 136.0,
      "text": "گجامی ری گجامی آی با چی یا کی هاست"
    },
    {
      "start": 136.0,
      "end": 138.0,
      "text": "خبر کردی"
    },
    {
      "start": 138.0,
      "end": 142.0,
      "text": "که چی دست با ورد نمیشدار ام جدی میگن"
    },
    {
      "start": 142.0,
      "end": 144.0,
      "text": "گزاشت او با سر امان"
    },
    {
      "start": 144.0,
      "end": 148.0,
      "text": "چی چی میخوای هاست دل امان"
    },
    {
      "start": 148.0,
      "end": 150.0,
      "text": "خیلی چیزا تصیر تا"
    },
    {
      "start": 150.0,
      "end": 152.0,
      "text": "گزی چیزا تصیر امان"
    },
    {
      "start": 152.0,
      "end": 156.0,
      "text": "گزاشت او با سر امان"
    },
    {
      "start": 156.0,
      "end": 158.0,
      "text": "دیدی چیو ما سر امان"
    },
    {
      "start": 158.0,
      "end": 160.0,
      "text": "ها چی دل امان"
    },
    {
      "start": 160.0,
      "end": 164.0,
      "text": "سا پوره خالی دورا بر امان"
    },
    {
      "start": 164.0,
      "end": 166.0,
      "text": "گفتم جاو شبت شده"
    },
    {
      "start": 166.0,
      "end": 168.0,
      "text": "دور تر شده"
    },
    {
      "start": 168.0,
      "end": 172.0,
      "text": "ایک آشلال بودم وارفی"
    },
    {
      "start": 172.0,
      "end": 174.0,
      "text": "نمیزده"
    },
    {
      "start": 174.0,
      "end": 176.0,
      "text": "امان"
    },
    {
      "start": 176.0,
      "end": 180.0,
      "text": "هاول مراه از شره رای ام بون نخای بود"
    },
    {
      "start": 180.0,
      "end": 182.0,
      "text": "مهن"
    },
    {
      "start": 182.0,
      "end": 186.0,
      "text": "پوشت شدری که نخا هم گفت میمیره"
    },
    {
      "start": 186.0,
      "end": 188.0,
      "text": "اشخی که یه روزی به تو داده"
    },
    {
      "start": 188.0,
      "end": 190.0,
      "text": "مما گین که تو یه خابت بینه"
    },
    {
      "start": 190.0,
      "end": 192.0,
      "text": "با باره یه روزی منه تو ها"
    },
    {
      "start": 192.0,
      "end": 194.0,
      "text": "مما گین که زیر خابت بینه"
    },
    {
      "start": 194.0,
      "end": 198.0,
      "text": "با باره ام نمیشه یه شفه ابدم"
    },
    {
      "start": 198.0,
      "end": 202.0,
      "text": "به تون ام قادر بسشه"
    },
    {
      "start": 202.0,
      "end": 204.0,
      "text": "چه جوری شده تو دل تومت"
    },
    {
      "start": 204.0,
      "end": 206.0,
      "text": "منه به حول خراب بینه"
    },
    {
      "start": 206.0,
      "end": 208.0,
      "text": "وصد چیم ام دلش ناید"
    },
    {
      "start": 208.0,
      "end": 212.0,
      "text": "که تو ها از دلش بری"
    },
    {
      "start": 212.0,
      "end": 214.0,
      "text": "تو همه یه داشن هم بودی"
    },
    {
      "start": 214.0,
      "end": 218.0,
      "text": "رسیدان داشن هم بیکی"
    },
    {
      "start": 218.0,
      "end": 220.0,
      "text": "وصد چیم ام دلش ناید"
    },
    {
      "start": 220.0,
      "end": 222.0,
      "text": "دل تومش کنه یه کی"
    },
    {
      "start": 222.0,
      "end": 225.0,
      "text": "قلبی که ما لمن نشه"
    },
    {
      "start": 225.0,
      "end": 229.0,
      "text": "موهنی باوشه ما لیکی"
    },
    {
      "start": 229.0,
      "end": 232.0,
      "text": "دارفه کی غش کردی رفتی"
    },
    {
      "start": 232.0,
      "end": 235.0,
      "text": "ام اجیو کش کردی رفتی"
    },
    {
      "start": 235.0,
      "end": 237.0,
      "text": "بیدون سیها دم خرابه"
    },
    {
      "start": 237.0,
      "end": 240.0,
      "text": "ام بازلتش کردی رفتی"
    },
    {
      "start": 240.0,
      "end": 243.0,
      "text": "ایمندل ام اده سد تو دا دم"
    },
    {
      "start": 243.0,
      "end": 245.0,
      "text": "ام اتو لهش کردی رفتی"
    },
    {
      "start": 245.0,
      "end": 248.0,
      "text": "شدم اجیبا سم چه هم نم"
    },
    {
      "start": 248.0,
      "end": 251.0,
      "text": "دجارو به هش کردی رفتی"
    },
    {
      "start": 251.0,
      "end": 253.0,
      "text": "با از اشتا آبا سر مر"
    },
    {
      "start": 253.0,
      "end": 256.0,
      "text": "تو چی میخوای از دل مر"
    },
    {
      "start": 256.0,
      "end": 259.0,
      "text": "خیبی چیزا تقصیر تا"
    },
    {
      "start": 259.0,
      "end": 261.0,
      "text": "بازی چیزا تقصیر مر"
    },
    {
      "start": 261.0,
      "end": 264.0,
      "text": "تو سا اشتا آبا سر مر"
    },
    {
      "start": 264.0,
      "end": 267.0,
      "text": "دیری چیم اقصیر مر"
    },
    {
      "start": 267.0,
      "end": 270.0,
      "text": "و اجیده لحال سماره"
    }
  ]
}